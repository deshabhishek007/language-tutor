		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--custom-text)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--white)",
					"text": "var(--wp--preset--color--custom-accent)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--custom-text)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-text)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--white)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--custom-accent)"
				},
				"typography": {
